<!DOCTYPE html>
<html>
<head>
  <title>Crawler</title>
  <%= stylesheet_link_tag    'application', 'ion.rangeSlider', 'ion.rangeSlider.skinFlat', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "https://js.stripe.com/v2/", 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= tag :meta, :name => "stripe-key", :content => STRIPE_PUBLIC_KEY %>
</head>
<body>
	
<%= render 'nav/top_nav' %>

<div class="main_container fill">
	
	<% if current_user %>
		<div class="main_left_container fill left_nav_bg">
			<%= render 'nav/left_nav' %>
		</div>
	<% end %>
	
	<div class="main_right_container fill main_page_bg">
		<%= yield %>
	</div>

</div>

</body>
</html>
