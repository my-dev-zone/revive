<div class="main_page_container">
	
	<div class="container-fluid header_title_container">
		<div class="row">
			<div class="col-xs-12">
				<div class="header_title_content">
					<div class="header_glyph">
						<span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>
					</div>
					<div class="header_text">
						<p>Found Avalible Domains</p>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="container-fluid container_section">
	
		<div class="row">
		
			<div class="col-xs-12">
			
				<div class="row">
					
					<div class="panel panel-default">
						<div class="panel-heading panel_pink_heading">
							
						</div>
						<div class="panel-body">
							
							<table class="table table-hover table-striped">
								
								<thead>
									<tr>
										<!-- <th></th> -->
										<th>DOMAIN</th>
										<th>DA</th>
										<th>PA</th>
										<th>TF</th>
										<th>CF</th>
										<th>TF/CF</th>
										<th>RD</th>
										<th>BL</th>
										<th>FOUND ON</th>
									</tr>
								</thead>
								
								<tbody>
									
									<% if @processing %>
										
										<% @available.each do |i| %>
											<tr>
												<!-- <td>
													<span class="glyphicon glyphicon-trash glyph_pink" aria-hidden="true"></span>
													<span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>
												</td> -->
												<td><%= i.url %></td>
												<td>-</td>
												<td>-</td>
												<td>-</td>
												<td>-</td>
												<td>-</td>
												<td>-</td>
												<td>-</td>
												<td><%= i.found_on %></td>
											</tr>
										<% end %>
										
									<% else %>
									
										<% @available.each do |i| %>
											<tr>
												<!-- <td>
													<span class="glyphicon glyphicon-trash glyph_pink" aria-hidden="true"></span>
													<span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>
												</td> -->
												<td>www.<%= i.simple_url %></td>
												<td><%= number_with_precision(i.da, precision: 0) %></td>
												<td><%= number_with_precision(i.pa, precision: 0) %></td>
												<td><%= i.trustflow %></td>
												<td><%= i.citationflow %></td>
												<td><%= number_with_precision(i.trustflow.to_f/i.citationflow.to_f) %></td>
												<td><%= i.refdomains %></td>
												<td><%= i.backlinks %></td>
												<td><%= i.found_on %></td>
											</tr>
										<% end %>
									
									<% end %>
									
									
								</tbody>
								
							</table>
							
						</div>
					</div>
					
				</div>
			
			</div>
		
		</div>
	
	</div>

</div>